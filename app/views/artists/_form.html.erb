<%= form_for [@artist], :remote => true do |f| %>
  <%= render "layouts/errors", :object => @artist %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :nationality %>
  <%= f.text_area :nationality %>
  <%= f.label :year_of_birth %>
  <%= f.datetime_local_field :year_of_birth %>
  <%= f.submit %>
<% end %>
